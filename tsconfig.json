{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "." /* Base directory to resolve non-absolute module names. */,
    "paths": {
      "@components/*": ["./src/components/*"],
      "@components": ["./src/components"],
      "@utils/*": ["./src/utils/*"],
      "@utils": ["./src/utils"]
    }
  },
  "include": ["./App.tsx", "src/**/*", "src/type/index.d.ts", "custom.d.ts"]
}
